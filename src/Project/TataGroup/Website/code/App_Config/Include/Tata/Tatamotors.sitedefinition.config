<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<sites>
			<site name="tatamotors" patch:before="site[@name='website']"
				  virtualFolder="/"
				  physicalFolder="/"
				  rootPath="/sitecore/content/tata-motors"
				  startItem="/tata-motors-home"
				  hostName="tatamotors.local"
			      targetHostName="tatamotors.local"
				  database="web"
				  domain="extranet"
				  allowDebug="true"
				  cacheHtml="true"
				  htmlCacheSize="50MB"
				  enablePreview="true"
				  enableWebEdit="true"
				  enableDebugger="true"
				  disableClientData="false"/>
		</sites>
	</sitecore>
</configuration>